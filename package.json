{
  "name": "heimdall",
  "version": "0.9.1",
  "description": "An open-source password manager app.",
  "author": {
    "name": "Levente Balogh",
    "email": "balogh.levente.hu@gmail.com",
    "url": "leventebalogh.com"
  },
  "main": "./src/main.js",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.28.0",
    "ignore-loader": "^0.1.2",
    "jest": "^19.0.2",
    "lodash": "^4.17.4",
    "mousetrap": "^1.6.1",
    "node-sass": "^4.5.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-redux": "^5.0.4",
    "react-textarea-autosize": "^5.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.11",
    "sass-loader": "^6.0.5",
    "sinon": "^2.2.0",
    "store": "^2.0.4",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.9",
    "valid-url": "^1.0.9",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "winston": "^2.3.1",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "electron": "1.7.6",
    "electron-builder": "^19.22.1",
    "electron-packager": "^8.7.1",
    "eslint": "^4.7.0",
    "eslint-config-casumo": "^1.3.0",
    "eslint-plugin-react": "^7.3.0",
    "locate-path": "^2.0.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "scripts": {
    "electron": "./node_modules/.bin/electron .",
    "dev": "webpack --config ./webpack.config.js -w",
    "build": "webpack --config ./webpack.config.js",
    "test": "jest",
    "tdd": "jest --watchAll",
    "lint": "eslint ./src ./lib ./bin",
    "dist-mac": "yarn build && electron-builder -m --publish \"never\" && zip ./dist/osx.zip ./dist/mac"
  },
  "build": {
    "productName": "Heimdall",
    "appId": "com.electron.heimdall",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "files": [
      "build/",
      "src/main.js"
    ]
  },
  "keywords": [],
  "license": "ISC"
}
